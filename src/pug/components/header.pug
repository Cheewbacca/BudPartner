header
    a(href="/").logo
        include ../svg/header__logo
    nav#burger
        ul
            -let nav = new Map();
            -nav.set('услуги', 'uslugi.html').set('о нас', 'o-nas.html').set('контакты', 'kontakty.html');
            -for(let [key, value] of nav){
                li
                    a(href="" + value).main-link= key
                    img(src="static/img/arrow.svg", alt="arrow")
            -}
            li 
                ul.language
                    li 
                        a(href="#").main-link RU
                        img(src="static/img/triangle.svg", alt="triangle")
                    li 
                        a(href="#").main-link UA
                        img(src="static/img/triangle.svg", alt="triangle")